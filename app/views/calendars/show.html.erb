<p id="notice"><%= notice %></p>
<h1>Meetings</h1>

<%= month_calendar do |date| %>
  <% stamp = @calendar.stamps.find_by(date: date.to_datetime) %>

  <% if stamp.present? %>
    <%= link_to date.day, stamp_path(calendar_id: @calendar.id, date: date), method: :delete, class: '' %>
    checked!
  <% else %>
    <%= link_to date.day, stamps_path(calendar_id: @calendar.id, date: date), method: :post, class: '' %>
  <% end %>
<% end %>
<%= link_to "TOP", root_path %>
